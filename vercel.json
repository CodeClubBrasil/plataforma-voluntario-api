{
    "version": 2,
    "builds": [
      { "src": "npm run build", "use": "@vercel/static-build" },
      { "src": "src/main.ts", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      }
    ]
  }
  