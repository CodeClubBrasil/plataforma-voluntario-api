{
    "version": 2,
    "name": "plataforma-voluntario-api",
    "outputDirectory": "dist",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ]
}
